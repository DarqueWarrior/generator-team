{
   "name": "generator-team",
   "version": "0.4.10",
   "description": "Generates an app with CI/CD in Team Foundation Server or Team Services",
   "main": "./generators/app/index.js",
   "scripts": {
      "test": "node node_modules/istanbul/lib/cli.js cover --report cobertura --report lcov node_modules/mocha/bin/_mocha -- -R xunit -O output=unit.xml unit/test",
      "unittest": "node node_modules/istanbul/lib/cli.js cover --report cobertura --report lcov node_modules/mocha/bin/_mocha -- -R xunit -O output=unit.xml unit/test",
      "inttest": "node node_modules/istanbul/lib/cli.js cover --report cobertura --report lcov node_modules/mocha/bin/_mocha -- -R xunit -O output=unit.xml integration/test"
   },
   "files": [
      "generators/app",
      "generators/asp",
      "generators/aspFull",
      "generators/azure",
      "generators/build",
      "generators/docker",
      "generators/git",
      "generators/k8s",
      "generators/java",
      "generators/node",
      "generators/pipeline",
      "generators/profile",
      "generators/project",
      "generators/registry",
      "generators/release",
      "generators/powershell",
      "generators/nuget",
      "generators/feed"
   ],
   "keywords": [
      "yeoman-generator",
      "tfs",
      "vsts",
      "java",
      ".net Core",
      "PowerShell",
      "Azure",
      "Node",
      "Node.js",
      "Team Foundation Server",
      "Team Services",
      "Visual Studio",
      "DevOps",
      "RubDevOpsOnIt",
      "Blackshirt"
   ],
   "author": "@DonovanBrown",
   "repository": {
      "type": "git",
      "url": "https://github.com/DarqueWarrior/generator-team"
   },
   "license": "MIT",
   "dependencies": {
      "async": "^2.6.1",
      "node-env-file": "^0.1.8",
      "pad": "^2.2.1",
      "request": "^2.88.0",
      "uuid": "^3.3.2",
      "yeoman-generator": "^3.1.1",
      "cheerio": "^1.0.0-rc.2",
      "yosay": "^2.0.2"
   },
   "devDependencies": {
      "azure-arm-resource": "^3.1.1-preview",
      "fs-extra": "^5.0.0",
      "istanbul": "*",
      "jshint": "^2.9.6",
      "mocha": "^5.2.0",
      "ms-rest-azure": "^2.5.7",
      "proxyquire": "^2.1.0",
      "sinon": "^4.5.0",
      "sinon-test": "^2.3.0",
      "yeoman-assert": "^3.1.1",
      "yeoman-test": "^1.8.0",
      "yo": "^2.0.5"
   }
}
