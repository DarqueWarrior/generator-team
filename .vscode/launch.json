{
   // Use IntelliSense to learn about possible Node.js debug attributes.
   // Hover to view descriptions of existing attributes.
   // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
   "version": "0.2.0",
   "configurations": [
      {
         "type": "node",
         "request": "launch",
         "name": "Launch Program",
         "program": "${file}"
      },
      {
         "type": "node",
         "request": "launch",
         "name": "yo team",
         "program": "${workspaceRoot}/node_modules/yo/lib/cli.js",
         "args": ["team"],
         "stopOnEntry": true,
         "cwd": "${workspaceRoot}",
         "console": "integratedTerminal",
         "internalConsoleOptions": "neverOpen"        
      },
      {
         "type": "node",
         "request": "launch",
         "name": "yo team:pipeline",
         "program": "${workspaceRoot}/node_modules/yo/lib/cli.js",
         "args": ["team:pipeline"],
         "stopOnEntry": true,
         "cwd": "${workspaceRoot}",
         "console": "integratedTerminal",
         "internalConsoleOptions": "neverOpen"        
      },
      {
         "name": "LaunchMocha",
         "type": "node",
         "request": "launch",
         "program": "${workspaceRoot}/node_modules/mocha/bin/_mocha",
         "stopOnEntry": false,
         "args": [
            "--debug-brk",
            "--timeout",
            "600000"
         ],
         "cwd": "${fileDirname}/..",
         "preLaunchTask": null,
         "runtimeExecutable": null,
         "runtimeArgs": [
            "--nolazy"
         ],
         "env": {
            "NODE_ENV": "development"
         },
         "externalConsole": false,
         "sourceMaps": false,
         "outDir": "${workspaceRoot}/temp"
      },
      {
         "type": "node",
         "request": "attach",
         "name": "Attach to Port",
         "address": "localhost",
         "port": 5858
      }
   ]
}