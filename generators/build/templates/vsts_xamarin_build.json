    },
    "androidCertificatPwd": {
      "value": null,
      "isSecret": true
    },
    "androidversionCode": {
      "value": ""
    },
    "BuildConfiguration": {
      "value": "Release",
      "allowOverride": true
    },
    "Identity": {
      "value": "iPhone Developer:()"
    },
    "iOSCertificatPwd": {
      "value": null,
      "isSecret": true
    },
    "UUID": {
      "value": "d795fbf1-c7ef-4b96-ac38-d57dcf780d02"
