    "type": "TfsGit",
    "name": "{{Project}}",
    "url": "https://{{VSTS}}.visualstudio.com/_git/{{Project}}",
    "defaultBranch": "refs/heads/master",
    "clean": "false",
    "checkoutSubmodules": false
