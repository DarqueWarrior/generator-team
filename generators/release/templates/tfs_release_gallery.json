                  {
                     "taskId": "e213ff0f-5d5c-4791-802d-52ea3e7be1f1",
                     "version": "1.*",
                     "name": "Publish to Dev Gallery",
                     "enabled": true,
                     "alwaysRun": false,
                     "continueOnError": false,
                     "timeoutInMinutes": 0,
                     "definitionType": "task",
                     "overrideInputs": {},
                     "condition": "succeeded()",
                     "inputs": {
                        "scriptType": "filePath",
                        "scriptName": "$(System.ArtifactsDirectory)\\{{BuildName}}\\drop\\{{ProjectName}}\\Release\\{{ProjectName}}\\build.ps1",
                        "arguments": "-Publish -GalleryUri $(GalleryUri) -NugetApiKey $(NugetApiKey) -ReleaseDirectoryBase $(System.ArtifactsDirectory)\\{{BuildName}}\\drop\\{{ProjectName}}\\Release",
                        "workingFolder": "$(System.DefaultWorkingDirectory)/{{BuildName}}/drop/{{ProjectName}}",
                        "inlineScript": "",
                        "failOnStandardError": "true"
                     }
                  }
               ]
            }
         ],
         "environmentOptions": {
            "emailNotificationType": "OnlyOnFailure",
            "emailRecipients": "release.environment.owner;release.creator",
            "skipArtifactsDownload": false,
            "timeoutInMinutes": 0,
            "enableAccessToken": false,
            "publishDeploymentStatus": false
         },
         "demands": [],
         "conditions": [
            {
               "name": "ReleaseStarted",
               "conditionType": 1,
               "value": ""
